<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".RateFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="@dimen/rate_icon_size"
            android:layout_height="@dimen/rate_icon_size"
            android:src="@drawable/emoji_love" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="@dimen/rate_icon_size"
            android:layout_height="@dimen/rate_icon_size"
            android:src="@drawable/emoji_cool" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="@dimen/rate_icon_size"
            android:layout_height="@dimen/rate_icon_size"
            android:src="@drawable/emoji_happy" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="@dimen/rate_icon_size"
            android:layout_height="@dimen/rate_icon_size"
            android:src="@drawable/emoji_sad" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="@dimen/rate_icon_size"
            android:layout_height="@dimen/rate_icon_size"
            android:src="@drawable/emoji_angry" />

    </LinearLayout>
</layout>